#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//bin/bash" ]; then
  "$basedir//bin/bash"  "$basedir/../@blueprintjs/core/scripts/upgrade-blueprint-3.0.0-rename.sh" "$@"
  ret=$?
else 
  /bin/bash  "$basedir/../@blueprintjs/core/scripts/upgrade-blueprint-3.0.0-rename.sh" "$@"
  ret=$?
fi
exit $ret
