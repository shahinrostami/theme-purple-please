{
  "name": "@snyk/snyk-docker-pull",
  "description": "CLI-less docker pull",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --verbose --runInBand",
    "lint": "prettier --check '**/*.ts' && eslint 'src/**/*.ts' && (cd test && eslint '**/*.ts')",
    "format": "prettier --loglevel warn --write '**/*.ts'",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/docker-pull.git"
  },
  "author": "snyk.io",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@types/node": "^10.17.14",
    "@types/tar-stream": "^2.1.0",
    "@types/tmp": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.6.0",
    "glob": "^7.1.6",
    "jest": "^25.1.0",
    "mkdir-recursive": "^0.4.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@snyk/docker-registry-v2-client": "^2.3.0",
    "child-process": "^1.0.2",
    "tar-stream": "^2.2.0",
    "tmp": "^0.2.1"
  },
  "version": "3.7.0"
}
