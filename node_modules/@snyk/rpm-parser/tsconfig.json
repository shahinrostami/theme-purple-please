{
  "compilerOptions": {
    "outDir": "./dist",
    "pretty": true,
    "target": "es2017",
    "lib": [
        "es2017",
    ],
    "module": "commonjs",
    "sourceMap": true,
    "declaration": true,
    "importHelpers": true,
    "strict": true,
    "downlevelIteration": true,
    "noUnusedLocals": true,
    "noImplicitAny": true
  },
  "include": [
    "./lib/**/*"
  ]
}
